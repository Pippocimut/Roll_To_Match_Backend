{
  "name": "cloud-computing-project",
  "version": "1.0.0",
  "description": "A project for my cloud computing class of 2024 in JS",
  "main": "built/app.js",
  "scripts": {
    "dev": "concurrently \"tsc --build --watch\" \"npx tailwindcss-cli build -i ./public/css/style.css -o ./public/css/output.css --watch\" \"nodemon built/app.js\"",
    "start-tsc": "tsc --build --watch & npx tailwindcss-cli build -i ./public/css/style.css -o ./public/css/output.css --watch & nodemon built/app.js",
    "start-tailwind": "npx tailwindcss-cli build -i ./public/css/style.css -o ./public/css/output.css --watch",
    "start-nodemon": "nodemon built/app.js",
    "start": "npm run start-tsc && npm run start-tailwind && npm run start-nodemon",
    "start-docker": "docker image  build -t roll-to-match:1 . && docker container run -d --name roll-to-match --publish 80:3000 roll-to-match:1",
    "restart-docker": "docker container stop Staging && docker container rm Staging && docker image  build -t roll-to-match:1 . && docker container run -d --name Staging --publish 80:3000 roll-to-match:1",
    "build": "tsc --build",
    "database": "ts-node ./src/testing/jobs/main.ts",
    "test": "jest"
  },
  "module": "NodeNext",
  "moduleResolution": "nodenext",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
