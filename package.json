    "@types/connect-mongo": "^3.1.2",
    "@types/ejs": "^3.1.5",
    "@types/firebase": "^2.4.32",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "firebase": "^11.1.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.8.3",
    "mongoose-findorcreate": "^4.0.0",
    "nodemon": "^3.1.7",
